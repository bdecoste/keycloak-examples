ecure-deployment name="app-profile-jee-saml.war">
    <SP entityID="app-profile-jee-saml" sslPolicy="EXTERNAL">
        <Keys>
            <Key signing="true" encryption="true">
                <KeyStore file="/etc/sso-saml-secret-volume/keystore.jks" password="mykeystorepass">
                    <PrivateKey alias="jboss" password="mykeystorepass"/>
                    <Certificate alias="jboss"/>
                </KeyStore>
            </Key>
        </Keys>
        <IDP entityID="idp" signatureAlgorithm="RSA_SHA256" signatureCanonicalizationMethod="http://www.w3.org/2001/10/xml-exc-c14n#">
            <SingleSignOnService signRequest="true" validateResponseSignature="true" requestBinding="POST" bindingUrl="https://secure-sso-demo.cloudapps.example.com/auth/realms/demo/protocol/saml"/>
            <SingleLogoutService validateRequestSignature="true" validateResponseSignature="true" signRequest="true" signResponse="true" requestBinding="POST" responseBinding="POST" postBindingUrl="https://secure-sso-demo.cloudapps.example.com/auth/realms/demo/protocol/saml" redirectBindingUrl="https://secure-sso-demo.cloudapps.example.com/auth/realms/demo/protocol/saml"/>
            <Keys>
                <Key signing="true" >
                    <CertificatePem></CertificatePem>
                </Key>
            </Keys>
        </IDP>
    </SP>
</secure-deployment>
